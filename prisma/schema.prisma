generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model banner {
  id         Int    @id @default(autoincrement()) @db.UnsignedInt
  uuid       String @default("") @db.Char(36)
  status     String @default("") @db.VarChar(10)
  category   String @default("") @db.VarChar(10)
  title      String @default("") @db.VarChar(40)
  comment    String @db.MediumText
  datime     String @default("") @db.VarChar(20)
  data_url   String @db.MediumText
  source_url String @default("") @db.VarChar(250)
}

model browse_journal {
  id               Int    @id @default(autoincrement()) @db.UnsignedInt
  common_user_id   Int    @default(0) @db.UnsignedInt
  common_user_uuid String @default("") @db.Char(36)
  data_id          Int    @default(0) @db.UnsignedInt
  data_uuid        String @default("") @db.Char(36)
  category         String @default("") @db.VarChar(20)
  datime           String @default("") @db.VarChar(20)

  @@index([common_user_id], map: "browse_journal_common_user_id_IDX")
  @@index([data_id], map: "browse_journal_data_id_IDX")
}

model bulletin {
  id       Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid     String   @default("") @db.Char(36)
  title    String   @default("") @db.VarChar(40)
  dday     DateTime @default(dbgenerated("1900-01-01")) @db.Date
  receiver String   @default("") @db.VarChar(10)
  doc      Json
}

model campus {
  id             Int     @id @default(autoincrement()) @db.UnsignedInt
  uuid           String  @default("") @db.Char(36)
  mis_user_id    Int     @default(0) @db.UnsignedInt
  category       String? @default("") @db.VarChar(3)
  title          String  @default("") @db.VarChar(50)
  date           String  @default("") @db.VarChar(10)
  time           String  @default("") @db.VarChar(10)
  address_level1 String  @default("") @db.VarChar(20)
  address_level2 String  @default("") @db.VarChar(10)
  address_level3 String  @default("") @db.VarChar(10)
  address_level4 String  @default("") @db.VarChar(50)
  school         String  @default("") @db.VarChar(20)
  content        String  @db.MediumText
  mp             Json?

  @@index([date], map: "campus_date_IDX")
}

model captcha {
  id            Int    @id @default(autoincrement())
  email         String @default("") @db.VarChar(50)
  code          String @default("") @db.VarChar(6)
  datime        String @default("") @db.VarChar(20)
  user_id       Int    @default(0)
  user_category String @default("") @db.VarChar(4)
}

model common_data {
  id        Int    @id @default(autoincrement()) @db.UnsignedInt
  uuid      String @default("") @db.Char(36)
  master_id Int    @default(0) @db.UnsignedInt
  category  String @default("") @db.VarChar(10)
  name      String @default("") @db.VarChar(50)
  comment   String @default("") @db.VarChar(100)
}

model common_user {
  id          Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid        String   @default("") @db.Char(36)
  wx_openid   String   @default("") @db.VarChar(50)
  password    String   @default("") @db.Char(64)
  name        String   @default("") @db.VarChar(20)
  email       String   @default("") @db.VarChar(50)
  phone       String   @default("") @db.VarChar(20)
  salt        String   @default("") @db.VarChar(16)
  date_create DateTime @default(now()) @db.DateTime(0)

  @@index([name], map: "common_user_name_IDX")
}

model common_user_schedule {
  id             Int @id @default(autoincrement())
  campus_id      Int @default(0)
  common_user_id Int @default(0)
}

model delivery {
  id               BigInt @id @default(autoincrement())
  resume_id        Int    @default(0) @db.UnsignedInt
  resume_uuid      String @default("") @db.Char(36)
  recruitment_id   Int    @default(0) @db.UnsignedInt
  recruitment_uuid String @default("") @db.Char(36)
  datime           String @default("") @db.VarChar(20)
  status           String @default("已投递") @db.VarChar(5)

  @@index([recruitment_id], map: "delivery_recruitment_id_IDX")
  @@index([resume_id], map: "delivery_resume_id_IDX")
}

model edit_journal {
  id        Int    @id @default(autoincrement()) @db.UnsignedInt
  user_id   Int    @default(0) @db.UnsignedInt
  user_uuid String @default("") @db.Char(36)
  category1 String @default("") @db.VarChar(4)
  category2 String @default("") @db.VarChar(50)
  datime    String @default("") @db.VarChar(20)
  data_id   Int    @default(0)
  data_uuid String @default("") @db.Char(36)
  remark    String @default("") @db.VarChar(200)

  @@index([user_id], map: "edit_journal_user_id_IDX")
}

model enterprise {
  id               Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid             String   @default("") @db.Char(36)
  status           String   @default("待认证") @db.VarChar(3)
  name             String   @default("") @db.VarChar(100)
  yingyezhizhao    String   @default("") @db.VarChar(30)
  phone            String   @default("") @db.VarChar(20)
  faren            String   @default("") @db.VarChar(50)
  zhuceriqi        String   @default("") @db.VarChar(20)
  zhuziguimo       String   @default("") @db.VarChar(20)
  yuangongshuliang String   @default("") @db.VarChar(20)
  yingyezhizhao_tu String?  @db.MediumText
  address1         String   @default("") @db.VarChar(20)
  address2         String   @default("") @db.VarChar(20)
  address3         String   @default("") @db.VarChar(20)
  address4         String   @default("") @db.VarChar(50)
  industry         String   @default("") @db.VarChar(20)
  intro            String   @default("") @db.VarChar(800)
  url              String   @default("") @db.VarChar(100)
  date             DateTime @default(now()) @db.DateTime(0)
  subject          String   @default("") @db.VarChar(50)

  @@index([date], map: "enterprise__date")
  @@index([address1, address2, address3], map: "enterprise_address1_IDX")
  @@index([name], map: "enterprise_name_IDX")
}

model enterprise_user {
  id              Int     @id @default(autoincrement()) @db.UnsignedInt
  uuid            String  @default("") @db.Char(36)
  enterprise_id   Int     @default(0) @db.UnsignedInt
  enterprise_uuid String  @default("") @db.Char(36)
  email           String  @default("") @db.VarChar(50)
  password        String  @default("") @db.Char(64)
  name            String  @default("") @db.VarChar(30)
  phone           String  @default("") @db.VarChar(20)
  salt            String? @default("") @db.VarChar(16)

  @@index([enterprise_id], map: "enterprise_user_enterprise_id_IDX")
}

model enterprise_user111 {
  id              Int     @id @default(autoincrement()) @db.UnsignedInt
  uuid            String  @default("") @db.Char(36)
  enterprise_id   Int     @default(0) @db.UnsignedInt
  enterprise_uuid String  @default("") @db.Char(36)
  email           String  @default("") @db.VarChar(50)
  password        String  @default("") @db.Char(64)
  name            String  @default("") @db.VarChar(30)
  phone           String  @default("") @db.VarChar(20)
  salt            String? @default("") @db.VarChar(16)

  @@index([enterprise_id], map: "enterprise_user_enterprise_id_IDX")
}

model favorite {
  id        Int    @id @default(autoincrement()) @db.UnsignedInt
  user_id   Int    @default(0) @db.UnsignedInt
  user_uuid String @default("") @db.Char(36)
  data_id   Int    @default(0) @db.UnsignedInt
  data_uuid String @default("") @db.Char(36)
  category1 String @default("") @db.VarChar(10)
  category2 String @default("") @db.VarChar(10)
  datime    String @default("") @db.VarChar(20)

  @@index([user_id], map: "favorite_common_user_id_IDX")
  @@index([data_id], map: "favorite_data_id_IDX")
}

model feedback {
  id            Int    @id @default(autoincrement()) @db.UnsignedInt
  user_id       Int    @default(0) @db.UnsignedInt
  user_uuid     String @default("") @db.Char(36)
  user_category String @default("") @db.VarChar(4)
  status        String @default("未处理") @db.VarChar(10)
  content       String @db.Text
  datime        String @default("") @db.VarChar(20)
  category      String @default("") @db.VarChar(4)

  @@index([user_id], map: "feedback_common_user_id_IDX")
}

model job_fair {
  id      Int     @id @default(autoincrement())
  title   String  @default("") @db.VarChar(50)
  content String? @db.Text
  datime  String  @default("") @db.VarChar(19)
  status  String  @default("停用") @db.VarChar(2)
}

model login_journal {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  user_id  Int    @default(0) @db.UnsignedInt
  ip       String @default("0.0.0.0") @db.VarChar(20)
  address  String @default("") @db.VarChar(20)
  category String @default("") @db.VarChar(4)
  datime   String @default("0000-01-01 00:01") @db.VarChar(20)

  @@index([user_id], map: "login_journal_user_id_IDX")
}

model message {
  id             Int    @id @default(autoincrement())
  common_user_id Int    @default(0)
  ent_user_id    Int    @default(0)
  content        String @default("") @db.VarChar(200)
  datime         String @default("") @db.VarChar(20)
  category       String @default("") @db.VarChar(20)
  status         String @default("未读") @db.VarChar(2)

  @@index([common_user_id], map: "common_user_id_index")
  @@index([ent_user_id], map: "ent_user_id_index")
}

model mis_user {
  id       Int    @id @default(autoincrement()) @db.UnsignedInt
  uuid     String @default("") @db.Char(36)
  username String @default("") @db.VarChar(20)
  password String @default("") @db.Char(32)
  name     String @default("") @db.VarChar(20)

  @@index([username], map: "mis_user_username_IDX")
}

model offer {
  id             Int     @id @default(autoincrement())
  recruitment_id Int     @default(0)
  common_user_id Int     @default(0)
  address        String? @default("") @db.VarChar(200)
  mianshishijian String? @default("") @db.VarChar(20)
  luxian         String? @default("") @db.VarChar(200)
  remark         String  @default("") @db.VarChar(200)
  phone1         String? @default("") @db.VarChar(11)
  phone2         String? @default("") @db.VarChar(11)
  datime         String? @default("") @db.VarChar(20)
  status         String  @default("未读") @db.VarChar(2)
}

model recommend {
  id             Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid           String   @default("") @db.Char(36)
  category       String   @default("") @db.VarChar(20)
  title          String   @default("") @db.VarChar(50)
  date1          String   @default("") @db.VarChar(10)
  date2          String   @default("") @db.VarChar(10)
  address_level1 String   @default("") @db.VarChar(20)
  address_level2 String   @default("") @db.VarChar(10)
  publisher      String   @default("") @db.VarChar(20)
  qty            String   @default("") @db.VarChar(10)
  baomignfangshi String   @default("") @db.VarChar(20)
  content        String   @db.MediumText
  date_create    DateTime @default(now()) @db.DateTime(0)
}

model recruitment {
  id              Int      @id @default(autoincrement()) @db.UnsignedInt
  enterprise_id   Int      @default(0) @db.UnsignedInt
  enterprise_uuid String   @default("") @db.Char(36)
  name            String   @default("") @db.VarChar(100)
  qty             String   @default("") @db.VarChar(10)
  description     String   @db.Text
  requirement     String   @db.Text
  address1        String   @default("") @db.VarChar(20)
  address2        String   @default("") @db.VarChar(20)
  address3        String   @default("") @db.VarChar(20)
  date            String   @default("") @db.VarChar(10)
  salary1         Int      @default(0)
  salary2         Int      @default(0)
  education       String   @default("") @db.VarChar(10)
  category        String   @default("") @db.VarChar(2)
  status          String   @default("在招") @db.VarChar(2)
  industry        String   @default("") @db.VarChar(20)
  position        String   @default("") @db.VarChar(20)
  uuid            String   @default("") @db.Char(36)
  job_fair_id     Json?
  date_refresh    DateTime @default(now()) @db.DateTime(0)

  @@index([address1, address2, address3], map: "recruitment_address1_IDX")
  @@index([date_refresh], map: "recruitment_date_refresh_IDX")
  @@index([enterprise_id], map: "recruitment_enterprise_id_IDX")
  @@index([name], map: "recruitment_name_IDX")
}

model report {
  id            Int    @id @default(autoincrement()) @db.UnsignedInt
  user_id       Int    @default(0) @db.UnsignedInt
  user_uuid     String @default("") @db.Char(36)
  user_category String @default("") @db.VarChar(4)
  data_id       Int    @default(0) @db.UnsignedInt
  data_uuid     String @default("") @db.Char(36)
  category      String @default("") @db.VarChar(5)
  content       String @default("") @db.VarChar(200)
  datime        String @default("") @db.VarChar(20)

  @@index([user_id], map: "report_common_user_id_IDX")
  @@index([data_id], map: "report_data_id_IDX")
}

model resume {
  id              Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid            String   @default("") @db.Char(36)
  common_user_id  Int      @default(0) @db.UnsignedInt
  status          String   @default("保密") @db.VarChar(2)
  name            String   @default("") @db.VarChar(20)
  phone           String   @default("") @db.VarChar(11)
  email           String   @default("") @db.VarChar(50)
  gender          String   @default("") @db.VarChar(2)
  birthday        String   @default("") @db.VarChar(10)
  school          String   @default("") @db.VarChar(20)
  education       String   @default("") @db.VarChar(10)
  date_begin      String   @default("") @db.VarChar(10)
  date_end        String   @default("") @db.VarChar(10)
  major           String   @default("") @db.VarChar(20)
  qiwangzhiwei    String   @default("") @db.VarChar(50)
  qiwanghangye    String   @default("") @db.VarChar(50)
  address1        String   @default("") @db.VarChar(20)
  address2        String   @default("") @db.VarChar(20)
  address3        String   @default("") @db.VarChar(20)
  address4        String   @default("") @db.VarChar(50)
  yixiangchengshi String   @default("") @db.VarChar(20)
  ziwopingjia     String?  @db.Text
  career          Json?
  record          Json?
  date_create     DateTime @default(now()) @db.DateTime(0)
  date_update     DateTime @default(now()) @db.DateTime(0)

  @@index([common_user_id], map: "resume_common_user_id_IDX")
  @@index([education, yixiangchengshi], map: "resume_education_IDX")
}

model sys_message {
  id            Int     @id @default(autoincrement())
  user_id       Int     @default(0)
  user_category String  @default("") @db.VarChar(4)
  category      String  @default("") @db.VarChar(20)
  title         String  @default("") @db.VarChar(40)
  content       String? @db.MediumText
  datime        String  @default("") @db.VarChar(20)
  status        String  @default("未读") @db.VarChar(2)
}

model topic {
  id          Int      @id @default(autoincrement()) @db.UnsignedInt
  uuid        String   @default("") @db.Char(36)
  mis_user_id Int      @default(0) @db.UnsignedInt
  tag         String   @default("") @db.VarChar(10)
  title       String   @default("") @db.VarChar(50)
  date        DateTime @default(dbgenerated("0000-01-01")) @db.Date
  time        String   @default("") @db.VarChar(10)
  content     String   @db.MediumText

  @@index([date], map: "recommend_date_IDX")
}
